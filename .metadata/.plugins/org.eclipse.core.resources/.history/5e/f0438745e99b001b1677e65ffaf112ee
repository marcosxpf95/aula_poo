package Modelos;

import java.util.List;

public class Cliente extends Pessoa {
	private int Agencia;
	private Conta Conta;
	private List<Cartao> Cartoes; 

	
	public Cliente(String nome, String CPF, String telefone, Endereco endereco, String email, int agencia, Conta conta, List<Cartao> cartoes) {
		super(nome, CPF, telefone, endereco, email);
		Agencia = agencia;
		Conta = conta;
		Cartoes = cartoes;				
	}


	public int getAgencia() {
		return Agencia;
	}


	public void setAgencia(int agencia) {
		Agencia = agencia;
	}


	public Conta getConta() {
		return Conta;
	}


	public void setConta(Conta conta) {
		Conta = conta;
	}


	public List<Cartao> getCartoes() {
		return Cartoes;
	}


	public void setCartoes(List<Cartao> cartoes) {
		Cartoes = cartoes;
	}
	
}
